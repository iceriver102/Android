package com.example.appData;

public class reminderData {
	private int msg_id;
	public String title;
	public String content;
	private int status;

	public int getID() {
		return this.msg_id;
	}

	public void setID(int id) {
		if (id > 0)
			this.msg_id = id;
	}
	
	public void setStatus(int status){
		this.status=status;
	}
	public int getStatus(){
		return this.status;
	}
	
	@Override
	public String toString(){
		return "REMINDER[ id:"+this.msg_id+", title:"+this.title+", content:"+this.content+", status:"+this.status+"]";
	}

}
