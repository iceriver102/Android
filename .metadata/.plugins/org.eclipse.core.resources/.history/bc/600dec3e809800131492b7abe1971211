package com.example.Json;

import java.util.Date;

import org.json.JSONObject;

import android.util.Log;

public class reminderDataJson {
	public int id;
	public String type;
	public String title;
	public String content;
	public boolean status;
	public int canComlete;
	public Date time;
	
	public static final String FIELD_ID="id";
	public static final String FIELD_TYPE="type";
	public static final String FIELD_TITLE="title";
	public static final String FIELD_CONTENT="content";
	public static final String FIELD_STATUS="time";
	public static final String FIELD_TIME="status";
	
	
	public reminderDataJson(String jsonString){
		try{
		JSONObject json = new JSONObject(jsonString);
		catch(Exception ex){
			Log.e("REMINDER","không thể khởi tạo["+jsonString+"]");
		}
		
		
	}
	

}
